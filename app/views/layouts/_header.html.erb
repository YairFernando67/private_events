<header class="header-container">
  <nav>
    <div class="nav-title">
      <p>Private Events</p>
    </div>
    <ul>
      <%= link_to "Home" , root_path %>
      <%= link_to "Events" , events_path %>
      <% if logged_in? %>
      <%= link_to "My events" , current_user %>
        <%= link_to "Logout" , logout_path %>
      <% else %>
        <%= link_to "Login" , login_path %>
        <%= link_to "SignUp" , signup_path %>
      <% end %>
    </ul>
  </nav>
</header>